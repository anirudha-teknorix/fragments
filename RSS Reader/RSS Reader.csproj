<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net8.0-android</TargetFramework>
    <SupportedOSPlatformVersion>21</SupportedOSPlatformVersion>
    <RootNamespace>RSS_Reader</RootNamespace>
    <OutputType>Exe</OutputType>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <ApplicationId>com.example.android.rssreader</ApplicationId>
    <ApplicationVersion>1</ApplicationVersion>
    <ApplicationDisplayVersion>1.0</ApplicationDisplayVersion>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="Resources\layout-port\activity_rssfeed.xml" />
    <None Remove="Resources\layout\activity_rssfeed.xml" />
    <None Remove="Resources\layout\fragment_rssitem_detail.xml" />
    <None Remove="Resources\layout\fragment_rsslist_overview.xml" />
    <None Remove="Resources\values-port\config.xml" />
    <None Remove="Resources\values\config.xml" />
  </ItemGroup>
</Project>